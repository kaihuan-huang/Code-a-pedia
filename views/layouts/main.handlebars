<body class="d-flex flex-column h-100">
  <!-- bootstrap navbar -->
  <nav class="navbar navbar-expand-lg bg-light navbar-custom">
    <div class="container-fluid justify-content-between no-pad-right">
      <a class="navbar-brand" href="/"><i class="fa-solid fa-chevron-left"></i>Code-A-Pedia<i class="fa-solid fa-chevron-right"></i></a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav"
        aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse justify-content-end" id="navbarNav">
        <ul class="navbar-nav">
          <li class="nav-item">
            <a class="nav-link" href="/" href="">Home</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/yourDevelopers">Your Developers</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/codeword">Code-Word</a>
          </li>
          <li class="nav-item"><a class="nav-link " href="/dashboard">Code-E</a></li>
          {{#if logged_in}}
          <li class="nav-item"><a class="nav-link " href="/dashboard/new">Code-Add</a></li>
          <a class="btn btn-outline-primary me-1 logout-btn log-btn" id="logout-link" href="#" role="button">Log Out</a>
          {{else}}
          <a class="btn btn-outline-primary me-1 login-form login-btn log-btn" href="/login" role="button">Log In</a>
          </li>
          <li>
            <a class="btn btn-primary signup signup-btn" href="/signup" role="button">Sign Up</a>
          </li>
          {{/if}}
        </ul>
      </div>
    </div>
  </nav>
  <!-- bootstrap carousel for pictures/explanation -->
  <main class="col-auto">
  {{{body}}}
  </main>
  <div class="clear">
  <footer id="sticky-footer" class="py-4 bg-transparent text-white-50 footer-custom">
    <div class="container text-center">
      <small>Copyright &copy; <a href="https://github.com/pjsherpa/Code-a-pedia">Code-A-Pedia by team Will Work for
          Stock Grants</a></small>
    </div>
  </footer>
  </div>
  
    {{!-- highlight --}}
 <script src="//cdnjs.cloudflare.com/ajax/libs/highlight.js/11.6.0/highlight.min.js"></script>

